{
  "name": "@gmjs/js-project-generator",
  "version": "0.0.8",
  "description": "Project generator for JavaScript/TypeScript projects.",
  "author": {
    "name": "Goran MrÅ¾ljak",
    "email": "goran.mrzljak@gmail.com",
    "url": "https://mrzli.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzli/js-project-generator"
  },
  "main": "src/index.js",
  "bin": {
    "jsgen": "src/index.js"
  },
  "prettier": "@gmjs/prettier-config",
  "scripts": {
    "start:dev": "ts-node src/run.ts",
    "lint": "eslint --fix . && prettier --write .",
    "lint:nofix": "eslint . && prettier .",
    "test-only": "jest --passWithNoTests",
    "test": "npm run lint && npm run test-only",
    "build-only": "shx rm -rf ./dist && tsc --project tsconfig.lib.json",
    "build": "npm run test && npm run build-only",
    "pub-only": "node publish.js",
    "pub": "npm run build && npm run pub-only"
  },
  "dependencies": {
    "@gmjs/assert": "^0.0.2",
    "@gmjs/exec-observable": "^0.0.2",
    "@gmjs/fs-async": "^0.0.4",
    "@gmjs/fs-shared": "^0.0.2",
    "@gmjs/path": "^0.0.1",
    "ejs": "^3.1.9",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@gmjs/eslint-config": "^0.0.10",
    "@gmjs/jest-config": "^0.0.1",
    "@gmjs/prettier-config": "^1.0.2",
    "@gmjs/test-util": "^0.0.9",
    "@gmjs/tsconfig": "^1.0.0",
    "@jest/globals": "^29.5.0",
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.1",
    "@types/shelljs": "^0.8.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "glob": "^10.2.6",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.11.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
