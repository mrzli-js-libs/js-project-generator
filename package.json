{
  "name": "@gmjs/js-project-generator",
  "version": "0.0.26",
  "description": "Project generator for JavaScript/TypeScript projects.",
  "author": {
    "name": "Goran MrÅ¾ljak",
    "email": "goran.mrzljak@gmail.com",
