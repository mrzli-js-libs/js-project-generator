{
  "name": "@gmjs/js-project-generator",
  "version": "0.0.22",
  "description": "Project generator for JavaScript/TypeScript projects.",
  "author": {
    "name": "Goran MrÅ¾ljak",
    "email": "goran.mrzljak@gmail.com",
    "url": "https://mrzli.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzli/js-project-generator"
  },
  "type": "commonjs",
  "main": "./src/index.js",
  "bin": {
    "jsgen": "src/index.js"
  },
  "scripts": {
    "dev": "ts-node src/run/index.ts",
    "lint": "eslint --fix . && prettier --write .",
    "lint:nofix": "eslint . && prettier --check .",
    "test-only": "jest --passWithNoTests",
    "test": "pnpm run lint && pnpm run test-only",
    "clean": "shx rm -rf ./dist",
    "build-only": "pnpm run clean && tsc --project tsconfig.lib.json",
    "build": "pnpm run test && pnpm run build-only",
    "pac-only": "npmpub pack",
    "pac": "pnpm run build && pnpm run pac-only",
    "pub-only": "npmpub pub",
    "pub": "pnpm run pac && pnpm run pub-only"
  },
  "dependencies": {
    "@gmjs/array-transformers": "^0.0.3",
    "@gmjs/assert": "^0.0.6",
    "@gmjs/exec-observable": "^0.0.3",
    "@gmjs/fs-async": "^0.0.5",
    "@gmjs/fs-shared": "^0.0.3",
    "@gmjs/path": "^0.0.2",
    "ajv": "^8.14.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@gmjs/eslint-config": "^0.0.36",
    "@gmjs/npm-publish-cli": "^0.0.9",
    "@gmjs/test-util": "^0.0.14",
    "@jest/globals": "^29.7.0",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/shelljs": "^0.8.15",
    "eslint": "^9.3.0",
    "glob": "^10.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "type-fest": "^4.18.3",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
